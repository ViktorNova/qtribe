stepsequencerwidgetbase.cpp: stepsequencerwidgetbase.h stepsequencerwidgetbase.ui 
	$(UIC) stepsequencerwidgetbase.ui -i stepsequencerwidgetbase.h -o stepsequencerwidgetbase.cpp

stepsequencerwidgetbase.o: stepsequencerwidgetbase.cpp stepsequencerwidgetbase.h

moc_qtribe.o: moc_qtribe.cpp  qtribe.h sequencerCore.h \
