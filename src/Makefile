moc_stepsequencerwidgetbase.o: moc_stepsequencerwidgetbase.cpp  stepsequencerwidgetbase.h 

moc_qtribe.cpp: $(MOC) qtribe.h
	$(MOC) qtribe.h -o moc_qtribe.cpp

moc_stepsequencerwidget.cpp: $(MOC) stepsequencerwidget.h
	$(MOC) stepsequencerwidget.h -o moc_stepsequencerwidget.cpp

moc_stepsequencerwidgetbase.cpp: $(MOC) stepsequencerwidgetbase.h
